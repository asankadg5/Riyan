import Autodesk
from Autodesk.Revit.DB import *

clr.AddReference('RevitServices')
import RevitServices
from RevitServices.Persistence import DocumentManager
doc = DocumentManager.Instance.CurrentDBDocument

elements = UnwrapElement(IN[0]) if isinstance(IN[0],list) else [UnwrapElement(IN[0])]

instances=[FilteredElementCollector(doc).OfClass(RevitLinkInstance).ToElements().Find(lambda x : x.GetLinkDocument().Title == elem.Document.Title) for elem in elements]
names=[elem.Document.Title for elem in elements]

if isinstance(IN[0], list): OUT = instances,names
else: OUT = instances[0],names[0]</Script>
